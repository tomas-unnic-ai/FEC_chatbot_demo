{
  "name": "fec-chatbot-demo",
  "version": "1.0.0",
  "description": "Chatbot demo con frontend y funciones serverless en Vercel",
  "private": true,
  "workspaces": [
    "frontend"
  ],
  "scripts": {
    "install:all": "npm install && cd frontend && npm install",
    "dev:frontend": "cd frontend && npm run dev",
    "dev": "vercel dev",
    "build:frontend": "cd frontend && npm run build",
    "build": "npm run build:frontend"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  }
}
